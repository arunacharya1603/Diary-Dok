{
    "version": 2,
    "builds": [
      { "src": "backend/app.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)",
        "dest": "/backend/routes/user"
      },
      {
        "src": "/api/v2/(.*)",
        "dest": "/backend/routes/dairy"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/app.js"
      }
    ]
  }
  